    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^3.0.3",
    "fast-xml-parser": "^3.16.0",
    "gatsby": "^2.19.45",
    "gatsby-image": "^2.2.44",
    "gatsby-plugin-algolia": "^0.7.0",
